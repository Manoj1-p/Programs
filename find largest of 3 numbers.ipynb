    "#finding largest out of three numbers\n",
    "a=int(input())\n",
    "b=int(input())\n",
    "c=int(input())\n",
    "if(a>b and a>c):\n",
    "    print(a)\n",
    "elif(b>a and b>c):\n",
    "    print(b)\n",
    "elif(c>a and c>b):\n",
    "    print(c)\n",
    "elif(a==b or a==c or b==c):\n",
    "    print(\"equality found\")"
